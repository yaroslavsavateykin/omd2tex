{
  "parse": {
    "merge_elements": true
  },
  "export": {
    "search_dir": "~/Obsidian/",
    "search_ignore_dirs": [
      ".trash",
      ".obsidian",
      ".plugins",
      ".smart-env",
      ".reference-map"
    ],
    "makefile": true,
    "export_dir": "./test/",
    "branching_project": false
  },
  "frontmatter":{
    "parse": true
  },
  "beamer": {
    "divide_element": ["splitline", "headline"]
  },
  "paragraph": {
    "latinify": true,
    "latinify_probability": 0.05,
    "latinify_json": "",
    "formulas_json": ""
  },
  "preamble": {
    "create_preamble": true,
    "settings_json": ""
  },
  "file": {
    "parse": true,
    "max_file_recursion": 5,
    "pass_if_not_found": true,
    "divide_with_new_page": true
  },
  "headline": {
    "parse": true,
    "global_level_align": true,
    "numeration": true,
    "clean_markdown_numeration": false,
    "clean_all_highlight": true
  },
  "image": {
    "parse": true,
    "absolute_path_in_project_export": false,
    "copy_to_folder_in_project_export": true,
    "wh_aspect_borders": [0.6, 1.8],
    "default_width": "8cm",
    "default_height": "8cm"
  },
  "quote": {
    "parse": true,
    "max_quote_recursion": 5
  },

  "list": {
    "itemsep": "0pt"
  },
  "codeblock": {
    "parse": true
  },
  "fragment": {
    "splitline": {
      "parse": true,
      "width": "0.5pt"
    },
    "caption": {
      "parse": true
    }
  }
}
